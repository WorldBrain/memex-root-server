{
  "name": "memex-root-service",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --trace-warnings build/main.js",
    "dev": "better-npm-run dev",
    "devmon": "better-npm-run devmon",
    "prepare": "tsc",
    "prepare:watch": "tsc --watch",
    "typings": "typings",
    "claudia": "claudia",
    "prepare-and-test": "npm run prepare && mocha build/**/*.test.js",
    "test": "mocha --require ts-node/register 'src/**/*.test.ts'",
    "test:watch": "mocha -r ts-node/register 'src/**/*.test.ts' -R min --watch --watch-extensions ts",
    "test:watch-verbose": "mocha -r ts-node/register 'src/**/*.test.ts' --watch --watch-extensions ts",
    "deploy-assets:staging": "aws s3 sync --acl public-read --metadata Cache-Control=max-age=0 assets/build/ s3://staging.memex.link/assets/ && aws s3 sync --acl public-read --metadata Cache-Control=max-age=0 static/ s3://staging.memex.link/",
    "deploy-assets:production": "aws s3 sync --acl public-read --metadata Cache-Control=max-age=0 assets/build/ s3://memex.link/assets/ && aws s3 sync --acl public-read --metadata Cache-Control=max-age=0 static/ s3://memex.link/",
    "deploy-backend:staging": "eb deploy staging",
    "deploy-backend:production": "read -p 'Deploying production version, last chance to change your mind by pressing CTRL+C. Press ENTER to continue:' -n 1 -r && eb deploy production"
  },
  "betterScripts": {
    "devmon": {
      "command": "nodemon --trace-warnings --watch build build/main.js",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "dev": {
      "command": "node --trace-warnings build/main.js",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "test": {
      "command": "mocha build/**/*.test.js",
      "env": {
        "NODE_ENV": "dev"
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
